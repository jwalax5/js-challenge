{
  "name": "santa-app",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start:vite": "vite --port 3000",
    "start": "npm run start:vite & npm run start:nodejs",
    "start:nodejs": "node -r  dotenv/config ./server/server.js",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "url": "https://github.com/alj-devops/santa-app"
